
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option macaddr '10:fe:ed:50:28:ea'
	option hwmode '11ng'
	option htmode 'HT20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'DSSS_CCK-40'
	option disabled '0'
	option txpower '27'
	option country 'US'

config wifi-iface
	option network 'wwan'
	option ssid 'dvoyka'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option bssid '92:F6:52:FF:BA:DD'
	option key 'password_for_wifi'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'nolik'
	option network 'lan'
	option encryption 'psk2+ccmp'
	option key 'password_for_wifi'


